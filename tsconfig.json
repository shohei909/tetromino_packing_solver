{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "outDir": "pages/js",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "skipLibCheck": true,
  "sourceMap": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "pages/js"]
}
